{
  "name": "choose-your-activities",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "clean": "rm -rf ./dist",
    "compile": "npm run clean && gulp compile",
    "watch": "gulp watch",
    "lint": "eslint .",
    "html-lint": "gulp lint-all-html",
    "pretest": "npm run compile && eslint . && NODE_ENV=test npm run db-migrate && rm -rf test/screenshots/*.html",
    "test-mocha": "mocha --timeout 5000",
    "test": "NODE_ENV=test npm run test-mocha && NODE_ENV=test EXPRESS_BASE_PATH=/choose-your-activities npm run test-mocha && npm run html-lint",
    "db-migrate": "knex migrate:latest --knexfile db/knexfile.js",
    "db-migrate-rollback": "knex migrate:rollback --knexfile db/knexfile.js",
    "heroku-postbuild": "npm run compile",
    "create-migration": "knex migrate:make --knexfile db/knexfile.js $MIGRATION",
    "snyk-protect": "snyk protect",
    "check-content": "gulp check-content",
    "prepublish": "npm run snyk-protect"
  },
  "// NOTE": [
    "^Pinned dependencies are actually devDependencies, which are only",
    "included so that they are available in the Heroku build pipeline"
  ],
  "dependencies": {
    "@babel/core": "7.10.5",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.10.4",
    "ansi-colors": "4.1.1",
    "body-parser": "~1.18.2",
    "bookshelf": "^0.10.0",
    "browserify": "^13.0.1",
    "celebrate": "^4.0.1",
    "cheerio": "^1.0.0-rc.2",
    "classlist-polyfill": "^1.2.0",
    "consolidate": "~0.14",
    "cookie-parser": "~1.3",
    "debug": "~2.6.9",
    "domify": "1.4.0",
    "double-ended-queue": "^2.1.0-0",
    "express": "~4.16.0",
    "fancy-log": "1.3.3",
    "font-awesome": "^4.7.0",
    "govuk-elements-sass": "^1.1.4",
    "govuk_frontend_toolkit": "^4.10.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-debug": "^3.1.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rev": "^8.1.0",
    "gulp-rev-collector": "^1.2.3",
    "gulp-sass": "^5.0.0",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify": "^1.5.3",
    "gulp4-run-sequence": "^1.0.1",
    "helmet": "^3.5.0",
    "hogan.js": "~3.0",
    "i18n": "^0.8.2",
    "joi": "^10.2.2",
    "jquery": "^3.5.1",
    "jquery-migrate": "^3.3.1",
    "knex": "^0.19.5",
    "lmt-utils": "~0.1.4",
    "morgan": "^1.6.1",
    "node-sass": "^4.12.0",
    "npm-git-secrets": "^1.0.5",
    "pg": "^4.5.5",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "request-promise-core": "^1.1.1",
    "sass": "^1.22.10",
    "serve-favicon": "~2.4.5",
    "snyk": "^1.465.0",
